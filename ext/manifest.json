{
  "name": "Chronokinesis",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "HackMIT 2019 Project",
  "homepage_url": "https://github.com/kyleposluns",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch?*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ],
  "permissions": [ "activeTab"]
}